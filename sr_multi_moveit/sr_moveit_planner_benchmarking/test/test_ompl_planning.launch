<launch>
  <arg name="complex" default="false" />
  <arg name="visualization" default="true"/>

  <include file = "$(find sr_moveit_planner_benchmarking)/launch/benchmarking.launch">
    <arg name="planning" value="ompl"/>
    <arg name="complex" value="$(arg complex)"/>
    <arg name="visualization" value="$(arg visualization)"/>
  </include>

  <node if="$(arg complex)" name = "test_planner_complex" pkg = "sr_moveit_planner_benchmarking" type = "test_planners_complex.py"
    args = "20 ompl"
    output="screen"/>
  <node unless="$(arg complex)" name = "test_planner_simple" pkg = "sr_moveit_planner_benchmarking" type = "test_planners.py"
    args = "20 ompl"
    output="screen"/>

</launch>
