<launch>
    <arg name="robot_ip" default="192.168.1.1"/>
    <arg name="kinematics_config" default="$(find ur_calibration)/etc/ur10_b_calibration.yaml"/>

    <include file="$(find ur_robot_driver)/launch/ur10_bringup.launch">
       <arg name="robot_ip" value="$(arg robot_ip)"/>
       <arg name="kinematics_config" value="/home/user/my_robot_calibration.yaml"/>
   </include>
</launch>
